#new:Function $GetStarPieceCount {
  PUSH RA
  %A0 has script context - assume var to put count in is in first argument
  LW A1, C (A0) %get args
  LAB A2, 8010F29F %get star piece count
  JAL ~Func:set_variable
    LW A1, 0 (A1) %get first arg
  POP RA
  JR RA
  LI V0, 2
}