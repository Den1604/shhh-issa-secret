    Case  <  .Story:Ch3_StarSpritDeparted % FFFFFFF3
      Call  BindNpcInteract   ( .Npc:Self $Script_Interact_80254A58 )
      Call  BindNpcIdle       ( .Npc:Self $Script_Idle_80255500 )
    Default
      If  *GF_MAC01_CalculatorReturned  ==  .True
        Call  BindNpcInteract   ( .Npc:Self $Script_Interact_80254A58 )
