@ $Script_Main {
  Set   *GB_WorldLocation  .Location:ToadTown
  Call  SetSpriteShading  ( .Shading:None )
  Call  SetCamPerspective ( .Cam:Default 00000003  25`  90`  4096` )
  Call  SetCamBGColor     ( .Cam:Default  0`  0`  0` )
